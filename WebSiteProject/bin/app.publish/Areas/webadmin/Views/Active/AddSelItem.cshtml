<tr id="tr_@(ViewBag.index)" class="selitem" seqidx="@(ViewBag.seqindex)" adid="-1" adpath="">
    <td class="text-center">
        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
            <input type="checkbox" class="checkboxes">
            <span name="check_@(ViewBag.index)"></span>
        </label>
    </td>
    <td>
        <span class="required" style="display:none;color:red">請確實上傳！</span>
        <input type="file" class="form-control">
        <span style="color:red">建議檔案大小5MB內</span>
    </td>
    <td>
        <input class="form-control" type="text" value="" />
    </td>
</tr>
